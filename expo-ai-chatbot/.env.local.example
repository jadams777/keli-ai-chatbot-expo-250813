EXPO_PUBLIC_API_URL="http://localhost:3000"

# Environment variables for Expo AI Chatbot
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key - Required for AI chat functionality
EXPO_PUBLIC_OPENAI_API_KEY=sk-your_openai_api_key_here

# Supabase Configuration - Required for data storage
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Serper API Key - Required for factual search queries
# Get your API key from https://serper.dev/
EXPO_PUBLIC_SERPER_API_KEY=your_serper_api_key_here