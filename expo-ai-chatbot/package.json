{
  "name": "ai-summaries",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:reverse:3000": "adb reverse tcp:3000 tcp:3000",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios -d",
    "web": "expo start --web",
    "start:dev-client": "bun expo:prebuild && bun expo start --dev-client",
    "expo:prebuild": "expo prebuild --skip-dependency-update react-native,react",
    "expo:prebuild:android:clean": "npx expo prebuild -p android --clean",
    "expo:prebuild:ios:clean": "npx expo prebuild -p ios --clean",
    "clean:native": "bun clean && rm -rf ./android && rm -rf ./ios && rm -rf ./.expo",
    "clean:node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:all": "bun clean:native && bun clean:node-modules",
    "eas:build:dev:simulator:ios:local": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:simulator --platform ios --local",
    "eas:build:dev:emulator:android": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:emulator --platform android",
    "eas:build:dev:android": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development --platform android",
    "eas:build:staging:android": "cross-env NODE_ENV=staging APP_ENV=staging eas build --profile staging --platform android",
    "eas:build:dev:simulator:ios": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:simulator --platform ios",
    "eas:build:dev:ios": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development --platform ios",
    "eas:build:dev:device:ios": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:device --platform ios",
    "eas:build:dev:device:android": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:device --platform android",
    "eas:build:staging:ios": "cross-env NODE_ENV=staging APP_ENV=staging eas build --profile staging --platform ios"
  },
